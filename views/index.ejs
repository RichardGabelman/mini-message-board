<html>
  <head>
    <title>Homepage</title>
    <link rel="stylesheet" href="/styles.css">
  </head>
  <body>
    <%- include('navbar', {links: links}) %>
    <h1><%- title %></h1>
    <ul>
      <% messages.forEach((message) => { %>
        <div>
          <h2><%- message.user %></h2>
          <p><%- message.text %></p>
          <p><%- message.added %></p>
          <a href="<%- message.user%>/<%-message.text%>">Expand</a>
        </div>
      <% }); %>
    </ul>
  </body>
</html>